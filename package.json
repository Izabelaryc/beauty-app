{
  "dependencies": {
    "parcel": "^2.8.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.10.0"
  },
  "scripts": {
    "start": "parcel index.html public/images/*",
    "prebuild-dev": "shx rm -rf dist/*",
    "build-dev": "parcel build ./index.html public/images/* --no-optimize --public-url ./",
    "prebuild": "shx rm -rf dist/*",
    "build": "env-cmd -x parcel build ./index.html public/images/* --public-url \\$PUBLIC_URL",
    "push-gh-pages": "push-dir --dir=dist --branch=gh-pages --cleanup --verbose"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "process": "^0.11.10",
    "shx": "^0.3.2"
  }
}
